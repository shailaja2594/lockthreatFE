import { AfterViewInit, Renderer2, ElementRef, OnDestroy } from '@angular/core';
export declare abstract class DxReportingComponent<T> implements AfterViewInit, OnDestroy {
    private renderer;
    private platformId;
    bindingSender: any;
    callbacks: any;
    control: ElementRef;
    constructor(renderer: Renderer2, platformId: Object);
    abstract getControlOptions(): T;
    abstract getCallbacks(): any;
    abstract getKoBinding(options: T): any;
    ngAfterViewInit(): void;
    ngOnDestroy(): void;
    getEventRaiser(): (eventName: any, args?: any) => any;
    height: string;
    width: string;
    cssClass: string;
    rtl: boolean;
}
