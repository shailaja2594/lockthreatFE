import { Renderer2 } from '@angular/core';
import { DxReportingComponent } from '../dx-reporting-component';
import { DxReportDesignerRequestOptionsComponent } from './dxrd-request-options';
import { DxReportDesignerCallbacksComponent } from './dxrd-callbacks';
export declare class DxReportDesignerComponent extends DxReportingComponent<any> {
    koReportUrl: any;
    requestOptions: DxReportDesignerRequestOptionsComponent;
    callbacksComponent: DxReportDesignerCallbacksComponent;
    constructor(renderer: Renderer2, platformId: Object);
    getCallbacks(): DxReportDesignerCallbacksComponent;
    getKoBinding(options: any): any;
    getControlOptions(): {
        allowMDI: boolean;
        reportUrl: any;
        requestOptions: DxReportDesignerRequestOptionsComponent;
        callbacks: {
            designer: {};
            preview: {};
        };
    };
    GetDesignerModel(): any;
    GetPreviewModel(): any;
    allowMDI?: boolean;
    reportUrl: string;
}
